<form action="/readcsv" method="POST" enctype="multipart/form-data">
    <h3>leerExcel</h3>
    <input type="file" name="sampleFile" />
    <input type="submit" class="btn btn-primary" />
</form>

<form action="/profile" method="POST" enctype="multipart/form-data">
    <h3>Subir foto de perfil</h3>
    <input type="file" name="sampleFile" />
    <input type="submit" class="btn btn-primary" />
</form>

<div class="card">
    {{#each rows}}

    {{#if this.profile_image}}
        <img class="card__image" src="{{this.profile_image}}" alt="Perfil de usuario" />
    {{else}}
        <img class="card__image" src="/img/default.png" alt="Perfil de usuario" />
    {{/if}}
    <h1 class="card__title">{{this.name}}</h1>
    <div class="card__job">{{this.job_title}}</div>
    <p class="card__about">{{this.description}}</p>
    <button type="button" class="btn btn-primary">Ver perfil</button>
    {{/each}}
</div>